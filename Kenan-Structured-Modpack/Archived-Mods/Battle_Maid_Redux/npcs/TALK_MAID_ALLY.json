        "text": "Guard this position.",
        "condition": { "not": "is_by_radio" },
        "topic": "TALK_MAID_GUARD",
        "effect": "assign_guard"
      },
      {
        "text": "I want to assign you to work at this camp.",
        "condition": { "npc_at_om_location": "FACTION_CAMP_ANY" },
        "topic": "TALK_MAID_GUARD",
        "effect": "assign_camp"
      },
